DROP TABLE IF EXISTS Orders;

CREATE TABLE Orders (
  id SERIAL,
  product_name varchar(200) DEFAULT NULL,
  product_price varchar(20) DEFAULT NULL,
  purchase_by_name varchar(200) DEFAULT NULL,
  purchase_date varchar(20) DEFAULT NULL,
  PRIMARY KEY (id)
);

DROP TABLE IF EXISTS profile;

CREATE TABLE profile (
  id SERIAL,
  profile_name varchar(200) DEFAULT NULL,
  profile_picture_url varchar(200) DEFAULT NULL,
  PRIMARY KEY (id)
);
